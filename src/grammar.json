{
  "name": "cicada",
  "rules": {
    "source_file": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "stmt"
      }
    },
    "stmt": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "on_stmt"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "let_stmt"
        },
        {
          "type": "SYMBOL",
          "name": "func_call_expr"
        }
      ]
    },
    "on_stmt": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "on"
        },
        {
          "type": "PATTERN",
          "value": "[a-z.]+"
        }
      ]
    },
    "let_stmt": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "let"
        },
        {
          "type": "SYMBOL",
          "name": "ident"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "expr"
        }
      ]
    },
    "expr": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "ident"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "func_call_expr": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "ident"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "string_ident"
            }
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          }
        ]
      }
    },
    "number": {
      "type": "PATTERN",
      "value": "[0-9]+([0-9])?"
    },
    "string": {
      "type": "PATTERN",
      "value": "\".*\""
    },
    "string_ident": {
      "type": "PATTERN",
      "value": "\\S+"
    },
    "ident": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9._]*"
    },
    "comment": {
      "type": "PATTERN",
      "value": "#.*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

